{
  "name": "js-starterkit",
  "version": "1.0.0",
  "description": "JavaScript StarterKit",
  "scripts": {
    "prebuild": "npm run clean-dist",
    "build": "babel-node buildScripts/build.js",
    "build:testing": "NODE_ENV=testing npm run build",
    "build:staging": "NODE_ENV=staging npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "dist-server:testing": "NODE_ENV=testing babel-node buildScripts/distServer.js",
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel lint:watch open:src",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw --color webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "wait-for-mock": "babel-node buildScripts/waitForMock.js",
    "test:wait-for-mock": "run-s wait-for-mock test",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "start-mockapi": "json-server --watch server/mockAPI/db.json --port 3001",
    "clean-dist": "rimraf ./dist-testing ./dist-staging ./dist-production && mkdir dist-testing dist-staging dist-production"
  },
  "author": "Hani",
  "license": "MIT",
  "dependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "bluebird": "^3.4.7",
    "classnames": "^2.2.5",
    "file-loader": "^0.9.0",
    "intl": "^1.2.5",
    "loader-utils": "^0.2.16",
    "lodash": "^4.17.4",
    "material-ui": "^0.16.6",
    "multi-loader": "^0.1.0",
    "node-sass": "^4.1.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.1",
    "react-intl": "^2.2.2",
    "react-mui-speeddial": "0.0.6",
    "react-redux": "^5.0.1",
    "react-responsive": "^1.2.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-form": "^6.4.3",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.1.1",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs": "^0.8.6",
    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^17.0.3",
    "json-schema-faker": "^0.4.0",
    "json-server": "^0.9.4",
    "localtunnel": "^1.8.2",
    "npm-run-all": "^3.1.2",
    "nsp": "^2.6.2",
    "numeral": "^1.5.6",
    "object-assign": "^4.1.0",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.4.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "svg-react-loader": "^0.4.0-beta.2",
    "wait-for-port": "0.0.2",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-md5-hash": "0.0.5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(png|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "modulePaths": "<rootDir>/src"
  }
}